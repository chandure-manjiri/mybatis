        <typeAlias alias = "SubjectEntity" type = "mybatis.com.mybatis.Entity.SubjectEntity"/>
        <typeAlias alias = "TeacherEntity" type = "mybatis.com.mybatis.Entity.TeacherEntity"/>
    </typeAliases>

    <mappers>
        <mapper resource = "mapper/school/StudentMapper.xml"/>
        <mapper resource = "mapper/school/SubjectMapper.xml"/>
        <mapper resource = "mapper/school/TeacherMapper.xml"/>
    </mappers>

</configuration>